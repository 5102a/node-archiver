{
  "source": {
    "include": ["readme.md", "index.js", "lib"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/)"
  },
  "opts": {
    "recurse": true,
    "destination": "../archiver-website/docs/",
    "template": "node_modules/minami"
  },
  "plugins": [
    "plugins/markdown"
  ]
}